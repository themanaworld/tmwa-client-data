<?xml version="1.0"?>
<chars>
  <!-- hide hair color selection -->
  <haircolor min="1" max="13"/>
  <!-- hide hair style selection -->
  <hairstyle min="1" max="20"/>
  <!-- hide look selection -->
  <look min="1" max="1"/>
  <!-- stat selection. 48 / 2 = 24. You start with 1 in all of 6 -> need 24+6 = 30 -->
  <stat min="1" max="10" sum="30"/>
  <!-- hide race selection -->
  <race min="1" max="1"/>
</chars>
