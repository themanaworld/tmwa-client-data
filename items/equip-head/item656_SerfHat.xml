    <sprite gender="female">equipment/head/serf-female.xml</sprite>
    <replace sprite="hair">
      <item from="-5"/>
    </replace>
    <replace direction="upall" sprite="hair">
      <item from="-5" to="-5"/>
    </replace>
  </item>
</items>
